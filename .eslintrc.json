{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-undef": 0,
    "default-case": 2,
    "eqeqeq": 1,
    "curly": 1,
    "vars-on-top": 1,
    "array-bracket-spacing": 1,
    "block-spacing": 1,
    "brace-style": 1,
    "camelcase": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "func-style": 1,
    "key-spacing": 1,
    "keyword-spacing": 1,
    "linebreak-style": 1,
    "lines-around-comment": 1,
    "max-depth": 2,
    "max-len": 1,
    "max-nested-callbacks": 2,
    "max-params": 1,
    "max-statements": 1,
    "max-statements-per-line": 1,
    "new-parens": 1,
    "no-const-assign": 2,
    "no-lonely-if": 1,
    "no-negated-condition": 1,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-trailing-spaces": [1, {
      "skipBlankLines": true
    }],
    "no-unneeded-ternary": 1,
    "no-whitespace-before-property": 1,
    "object-curly-spacing": 1,
    "one-var": 1,
    "one-var-declaration-per-line": 1,
    "operator-assignment": 1,
    "padded-blocks": [1, {
      "blocks": "always"
    }],
    "prefer-const": 1,
    "quote-props": 1,
    "quotes": [1, "single"],
    "require-jsdoc": 2,
    "semi": [2, "never"],
    "semi-spacing": 1,
    "space-before-blocks": 1,
    "spaced-comment": 1
  }
}
